a=[[],[],[],[],[],[],[]]
for i in range(7):
    for j in range(7):
         if(j%2==1):
             a[j].append(0)
         if(i%2==1):
             a[j].append(0)
             
         else:
             a[j].append(1)

             


for i in range(len(a)):
    for j in range(len(a[0])):
        print(a[i][j],end = " ")
    print()
