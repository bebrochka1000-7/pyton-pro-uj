for i in range(9):
    for j in range(6):
         if(i%3==0):
             a[i].append("+")
         if(i%3==1):
             a[i].append("*")
         if(i%3==2):
             a[i].append("^")

         
   


for i in range(len(a)):
    for j in range(len(a[0])):
